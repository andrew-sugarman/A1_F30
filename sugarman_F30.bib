	month = oct,
	year = {2023},
	note = {Publisher: eLife Sciences Publications Limited},
	file = {Full Text PDF:/home/andrew/Zotero/storage/ABYJQEV5/Yakovlev et al. - 2023 - Quantitative Geometric Modeling of Blood Cells fro.pdf:application/pdf},
