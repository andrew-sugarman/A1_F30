@article{koyuncu_visual_2023,
	title = {Visual assessment of {2D} levels within {3D} pathology datasets of prostate needle biopsies reveals substantial spatial heterogeneity},
	issn = {1530-0307},
	doi = {10.1016/j.labinv.2023.100265},
	abstract = {Prostate cancer (PCa) prognostication largely relies upon visual assessment of a few thinly sectioned biopsy specimens under a microscope to assign a Gleason Grade Group (GG). Unfortunately, the assigned GG is not always associated with a patient's outcome, in part due to the limited sampling of spatially heterogeneous tumors achieved by 2D histopathology. In this study, open-top light sheet (OTLS) microscopy was used to obtain 3D pathology datasets that were assessed by four human readers. Intra-biopsy variability was assessed by asking readers to perform Gleason grading of 5 different levels per biopsy for a total of 20 core-needle biopsies (i.e., 100 total images). Intra-biopsy variability (Cohen's kappa) was calculated as the worst pairwise agreement in GG between individual levels within each biopsy, and was found to be 0.34, 0.34, 0.38, and 0.43 for the four pathologists. These preliminary results reveal that even within a 1-mm diameter needle core, GG based on 2D images can vary dramatically depending upon the location within a biopsy being analyzed. We believe that morphological assessment of whole biopsies in 3D has the potential to enable more reliable and consistent tumor grading.},
	language = {eng},
	journal = {Laboratory Investigation; a Journal of Technical Methods and Pathology},
	author = {Koyuncu, Can and Janowczyk, Andrew and Farre, Xavier and Pathak, Tilak and Mirtti, Tuomas and Fernandez, Pedro L. and Pons, Laura and Reder, Nicholas P. and Serafin, Robert and Chow, Sarah Sl and Viswanathan, Vidya S. and Glaser, Adam K. and True, Lawrence D. and Liu, Jonathan Tc and Madabhushi, Anant},
