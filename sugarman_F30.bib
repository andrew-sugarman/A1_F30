	pmid = {32949099},
	pmcid = {PMC7782084},
	keywords = {Female, Humans, Male, cell morphology, Breast Neoplasms, Cell Count, Cell Line, Tumor, Cell Nucleus, cell size, Cell Size, circulating tumor cells, Cohort Studies, Colonic Neoplasms, histology, Indoles, Keratins, metastatic cancer, Neoplastic Cells, Circulating, Prostatic Neoplasms, single cell isolation methods, Urinary Bladder Neoplasms},
