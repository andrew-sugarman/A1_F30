	pmcid = {PMC5352383},
	keywords = {Female, Humans, Male, Melanoma, Skin Neoplasms, Biomarkers, Tumor, BRAF, DNA Mutational Analysis, Gene Frequency, Genetic Predisposition to Disease, heterogeneity, High-Throughput Nucleotide Sequencing, immunohistochemistry, Immunohistochemistry, Middle Aged, Mutation, Observer Variation, Phenotype, PNA-clamping, Predictive Value of Tests, primary melanoma, Prognosis, Proto-Oncogene Proteins B-raf, Real-Time Polymerase Chain Reaction, Reproducibility of Results},
	pages = {8069--8082},
	file = {Full Text:/home/andrew/Zotero/storage/WFTEKNRA/Bruno et al. - 2017 - Heterogeneity and frequency of BRAF mutations in p.pdf:application/pdf},
}

@article{teixido_molecular_2021,
	title = {Molecular {Markers} and {Targets} in {Melanoma}},
	volume = {10},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2073-4409},
	url = {https://www.mdpi.com/2073-4409/10/9/2320},
	doi = {10.3390/cells10092320},
	abstract = {Melanoma develops as a result of several genetic alterations, with UV radiation often acting as a mutagenic risk factor. Deep knowledge of the molecular signaling pathways of different types of melanoma allows better characterization and provides tools for the development of therapies based on the intervention of signals promoted by these cascades. The latest World Health Organization classification acknowledged the specific genetic drivers leading to melanoma and classifies melanocytic lesions into nine distinct categories according to the associate cumulative sun damage (CSD), which correlates with the molecular alterations of tumors. The largest groups are melanomas associated with low-CSD or superficial spreading melanomas, characterized by frequent presentation of the BRAFV600 mutation. High-CSD melanomas include lentigo maligna type and desmoplastic melanomas, which often have a high mutation burden and can harbor NRAS, BRAFnon-V600E, or NF1 mutations. Non-CSD-associated melanomas encompass acral and mucosal melanomas that usually do not show BRAF, NRAS, or NF1 mutations (triple wild-type), but in a subset may have KIT or SF3B1 mutations. To improve survival, these driver alterations can be treated with targeted therapy achieving significant antitumor activity. In recent years, relevant improvement in the prognosis and survival of patients with melanoma has been achieved, since the introduction of BRAF/MEK tyrosine kinase inhibitors and immune checkpoint inhibitors. In this review, we describe the current knowledge of molecular pathways and discuss current and potential therapeutic targets in melanoma, focusing on their clinical relevance of development.},
	language = {en},
	number = {9},
	urldate = {2023-06-12},
	journal = {Cells},
	author = {Teixido, Cristina and Castillo, Paola and Martinez-Vila, Clara and Arance, Ana and Alos, Llucia},
	month = sep,
	year = {2021},
	note = {Number: 9
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {\textit{BRAF}, \textit{KIT}, \textit{NRAS}, \textit{NTRK}, markers, melanoma, molecular pathways, target therapy},
	pages = {2320},
	file = {Full Text PDF:/home/andrew/Zotero/storage/KTJC6AG7/Teixido et al. - 2021 - Molecular Markers and Targets in Melanoma.pdf:application/pdf},
}

